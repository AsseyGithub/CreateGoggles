{
    "schemaVersion": 1,
    "id": "creategoggles",
    "version": "${version}",
    "name": "${modName}",
    "description": "Adds Goggle helmets and armored backtanks",
    "authors": ["${author}"],
    "contact": {
        "issues": "https://github.com/Robocraft999/CreateGoggles/issues",
        "sources": "https://github.com/Robocraft999/CreateGoggles"
    },
    "license": "MIT",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.robocraft999.creategoggles.fabric.CreateGogglesFabric"
        ]
    },
    "mixins": [
        "creategoggles.mixins.json",
        "creategoggles-common.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "minecraft": ">=${minecraft_version}",
        "architectury": ">=${architectury_version}",
        "create": "*"
    }
}
